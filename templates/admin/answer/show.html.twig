{% extends 'layoutAdmin.html.twig' %}

{% block title %}Answer{% endblock %}

{% block body %}
    <h1>Answer</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ answer.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ answer.name }}</td>
            </tr>
            <tr>
                <th>GoodAnswer</th>
                <td>{{ answer.goodAnswer }}</td>
